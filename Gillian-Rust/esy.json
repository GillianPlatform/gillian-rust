{
  "name": "gillian-rust",
  "version": "0.0.0",
  "private": true,
  "description": "Rust instantiation for Gillian",
  "author": {
    "name": "Verified Trustworthy Software Specification Group at Imperial College",
    "url": "https://vtss.doc.ic.ac.uk"
  },
  "homepage": "GillianPlatform.github.io",
  "license": "BSD-3-Clause",
  "esy": {
    "build": "dune build @install --promote-install-files",
    "buildsInSource": "_build",
    "exportedEnv": {
      "GILLIAN_RUST_RUNTIME_PATH": {
        "scope": "global",
        "val": "#{self.share}/gillian-rust"
      }
    }
  },
  "scripts": {
    "watch": "dune build @install --watch",
    "format-check": "dune build @fmt",
    "format": "dune build @fmt --auto-promote",
    "test": "dune test -j 4",
    "clean": "dune clean"
  },
  "dependencies": {
    "@opam/cmdliner": "^1.0.4",
    "@opam/dune": "^3.6.1",
    "@opam/fmt": "^0.8.8",
    "@opam/ppx_deriving_yojson": "*",
    "@opam/ppxlib": ">=0.18.0",
    "@opam/printbox": "0.5",
    "@opam/yojson": "^1.7.0",
    "gillian": "GillianPlatform/Gillian:gillian.json#51d039008cc3e0db76a9955ad46791a4560e4315",
    "ocaml": "^4.14.x"
  },
  "devDependencies": {
    "ocaml": "4.14.x",
    "@opam/ocaml-lsp-server": "*",
    "@opam/odoc": "*",
    "@opam/ocamlformat": "0.20.1",
    "@opam/utop": "*"
  },
  "resolutions": {
    "@opam/conf-sqlite3": "ManasJayanth/esy-sqlite:esy.json#28dfc73a8eda790213a8359ad708231069ed7079"
  }
}